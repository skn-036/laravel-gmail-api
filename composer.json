{
    "name": "skn036/laravel-gmail-api",
    "version": "1.0.0",
    "description": "Wrapper around gmail api for laravel.",
    "keywords": [
        "Muhammad Sajedul Karim",
        "laravel",
        "laravel-gmail-api"
    ],
    "homepage": "https://github.com/skn036/laravel-gmail-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Sajedul Karim",
            "email": "noman0703036@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0",
        "skn036/laravel-google-client": "^1.0",
        "symfony/mailer": "^7.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Skn036\\Gmail\\": "src/",
            "Skn036\\Gmail\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skn036\\Gmail\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-gmail-api --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skn036\\Gmail\\GmailServiceProvider"
            ],
            "aliases": {
                "Gmail": "Skn036\\Gmail\\Facades\\Gmail"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
